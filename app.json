{
  "expo": {
    "name": "ORLQB",
    "slug": "orlqb-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist",
      "bundleIdentifier": "org.orlqb.RN_ORLQB_APP",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "ORLQB needs location access to provide turn-by-turn navigation to event locations and hangar meetings.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "ORLQB needs location access to provide turn-by-turn navigation to event locations and hangar meetings."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "googleServicesFile": "./google-services.json",
      "package": "org.orlqb.RN_ORLQB_APP",
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
